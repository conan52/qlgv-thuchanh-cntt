@model TKBThucHanh.Models.MonHoc

@{
    ViewBag.Title = "Môn học";
    ViewBag.PageSubTitle = "Thông tin môn học";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<fieldset>
    <div class="form-horizontal">
        <div class="form-group">
            @Html.LabelFor(model => model.TenMonHoc, new { @class = "control-label col-sm-2" })
            <div class="col-sm-5">
                @Html.TextBoxFor(model => model.TenMonHoc, new { @class = "form-control" ,@readonly=true})
                @Html.ValidationMessageFor(model => model.TenMonHoc)
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.ChuyenNganh, new { @class = "control-label col-sm-2" })
            <div class="col-sm-5">
                @Html.TextBoxFor(model => model.ChuyenNganh, new { @class = "form-control", @readonly = true })
                @Html.ValidationMessageFor(model => model.ChuyenNganh)
            </div>
        </div>
    </div>
</fieldset>
<p>
    @Html.ActionLink("Sửa thông tin môn học", "Edit", new { /* id=Model.PrimaryKey */ }) |
    @Html.ActionLink("Trở về trang danh sách Môn học", "Index")
</p>
